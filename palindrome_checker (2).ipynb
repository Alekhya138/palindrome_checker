      "source": [
        "n = input()\n",
        "n = n.lower()\n",
        "clean = \"\"\n",
        "for ch in n :\n",
        "  if ch.isalnum() :\n",
        "    clean += ch\n",
        "\n",
        "if clean == clean[::-1] :\n",
        "  print(\"palindrome\")\n",
        "else :\n",
        "  print(\"Not a palindrome\")"
      ],
